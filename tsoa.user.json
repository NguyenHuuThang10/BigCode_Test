{
  "entryFile": "src/start-up/main.ts",
  "noImplicitAdditionalProperties": "throw-on-extras",
  "controllerPathGlobs": ["src/controllers-user/**/*.ts"],
  "spec": {
    "outputDirectory": "src/routes/user/",
    "specVersion": 3,
    "basePath": "/api/user",
    "specFileBaseName": "swagger-user",
    "securityDefinitions": {
      "jwt": {
        "type": "http",
        "scheme": "bearer",
        "bearerFormat": "JWT"
      }
    }
  },
  "routes": {
    "routesDir": "src/routes/user",
    "authenticationModule": "src/middlewares/auth.middlewares.ts"
  }
}
